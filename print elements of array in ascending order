//WAP to print elemet in an array in ascending order.
#include<stdio.h>
#include<conio.h>
int main()
{
	int a[5]={5,10,15,20,25},i,j,temp;
	for(i=0;i<5;i++)
	{
		for(j=i+1;j<5;j++)
		{
			if(a[i]>a[j])
			{
				temp=a[i];
				a[i]=a[j];
				a[j]=temp;
			}
		}
		printf("The elements in ascending order is :\n");
		for(i=0;i<5;i++)
		{
			printf("%d\n",a[i]);
		}
		getch();
		return 0;
		}
	}
	
